{
  "version": 2,
  "builds": [
    {
      "src": "index.js", // or your main entry file (e.g., index.js/server.js)
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js", // matches your main entry file
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ]
}
